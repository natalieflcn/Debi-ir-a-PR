{
  "ignorePatterns": ["dist"],
  "extends": ["eslint:recommended", "react-app"],
  "plugins": ["react-refresh"],
  "env": {
    "browser": true,
    "es2020": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "no-unused-vars": ["warn", { "varsIgnorePattern": "^[A-Z_]" }]
  }
}
